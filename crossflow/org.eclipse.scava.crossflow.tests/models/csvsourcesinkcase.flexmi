<?nsuri org.eclipse.scava.crossflow?>
<?render-graphviz-dot graphviz.egl?>
<_>

	<!-- Simple example that adds pairs of numbers and prints the results -->
	<workflow name="BaseCase" package="org.eclipse.scava.crossflow.tests.csvsourcesinkcase">
		<csvsource name="NumberPairCsvSource" out="Additions" path="csvs/NumberPairCsvSource.csv"/>
		
		<queue name="Additions" type="NumberPair"/>
		<task name="Adder" in="Additions" out="AdditionResults"/>
		<queue name="AdditionResults" type="Number"/>
		
		<csvsink name="PrinterCsvSink" in="AdditionResults" path="csvs/AdditionResultsCsvSink.csv"/>
								
		<type name="NumberPair">
			<field name="a" type="int"/>
			<field name="b" type="int"/>
		</type>
		<type name="Number">
			<field name="n" type="int"/>
		</type>
	</workflow>	
	
</_>