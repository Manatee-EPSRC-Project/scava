[%
	var lang = s.eContainer().languages.select(l|l.name=="Java");
	var langExists = not lang.isEmpty;

	var package = s.eContainer().name.ftlc;
	
	if(langExists) package = lang.first.package;
%]
package [%=package%];

import javax.jms.Message;

public interface [%=s.name%]Consumer {
	
	void consume[%=s.name%]WithNotifications([%=s.type.name%] [%=s.type.name.ftlc()%]) throws Exception;
	
}